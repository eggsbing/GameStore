@model GameStore.Core.ViewModels.Users.UserDetailVm
@{
    ViewData["Title"] = "My Account";
}



<div class="container-fluid">
    <div class="row">
        <div class="col-md-6 h5">
            FullName : @Model.FullName
        </div>
        <div class="col-md-6 h5">
            Email : @Model.Email
        </div>
        <div class="col-md-6 h5">
            Mobile : @Model.Mobile
        </div>
        <div class="col-md-6 h5">
            Register Date : @Model.CreateDate
        </div>
    </div>
</div>


