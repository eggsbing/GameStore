@model IEnumerable<GameStore.Core.ViewModels.Notes.NoteIndexVm>
@{
    ViewData["Title"] = "Index";
}





@foreach (var item in Model)
{
    <div>
        <div class="container-fluid">
            <div>
                <h1 class="h1 text-center">@item.Name</h1>
            </div>
            <div>
                @item.Text
            </div>
        </div>
    </div>

    <div>
        <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-outline-warning">Edit</a> |
        <a asp-action="Index" asp-controller="Home" class="btn btn-outline-info btn-sm">Back to Dasdboard</a>
        <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-outline-danger">Delete</a>
    </div>
}


